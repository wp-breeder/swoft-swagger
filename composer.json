{
	"name": "wp-breeder/swoft-swagger",
	"type": "library",
	"license": "Apache-2.0",
	"keywords": [
		"php",
		"swoole",
		"swoft",
		"api doc",
		"swagger"
	],
	"description": "swagger component for swoft",
	"authors": [
		{
			"name": "wp-breeder",
			"email": "wp_breeder@163.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"ext-json": "*",
		"swoft/framework": "~2.0.1",
		"zircote/swagger-php": "^3.0"
	},
	"require-dev": {
		"swoft/swoole-ide-helper": "dev-master",
		"phpunit/phpunit": "^7.5",
		"swoft/http-server": "~2.0.1"
	},
	"autoload": {
		"psr-4": {
			"WpBreeder\\Swagger\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WpBreederTest\\Swagger\\Testing\\": "test/testing/"
		}
	},
	"scripts": {
		"test": "./vendor/bin/phpunit -c phpunit.xml"
	},
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://packagist.laravel-china.org"
		}
	}
}
